[
  // filenames matching these regexps will not be pushed to the database
  // uncomment to activate; separate entries with ","
  ".*~$",
  ".*\\.swp$",
  ".*\\.bak$",
  ".DS_Store",

  // ignore all data files and scripts
  "data$",
  "utils$",
  ".*\\.py$",

  // ignore readme files
  ".*\\.md$",

  // ignore bower files and .gitignore
  ".bowerrc",
  "bower.json",
  ".gitignore"
]
